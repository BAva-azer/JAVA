import java.util.*;
    public class main{
        public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
         System.out.println("your age :");
        int age=sc.nextInt();
         
        if
           (age>=18){
           System.out.println("eligible for vote");  

        }
        else
           {
            System.out.println("not eligible");

            
        }  
        
    }
    }
     
